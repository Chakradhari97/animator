{
  "version": "3.5.1",
  "configurations": [
    {
      "type": "chrome",
      "request": "attach",
      "port": 9222,
      "sourceMaps": true,
      "webRoot": "${workspaceFolder}",
      "name": "attach-renderer"
    },
    {
      "type": "node",
      "request": "attach",
      "sourceMaps": true,
      "port": 9221,
      "outFiles": [
        "${workspaceFolder}/packages/**/lib/**/*.js",
        "${workspaceFolder}/packages/**/lib/*.js"
      ],
      "name": "attach-main"
    },
    {
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceFolder}/packages/haiku-formats",
      "env": {
        "TS_NODE_PROJECT": "./tsconfig.all.json"
      },
      "runtimeArgs": [
        "-r",
        "ts-node/register",
        "-r",
        "tsconfig-paths/register"
      ],
      "args": [
        "./scripts/regenerate-goldens.ts"
      ],
      "name": "debug-regenerate-goldens"
    },
    {
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceFolder}/packages/haiku-formats",
      "env": {
        "TS_NODE_PROJECT": "./tsconfig.all.json"
      },
      "runtimeArgs": [
        "-r",
        "ts-node/register",
        "-r",
        "tsconfig-paths/register"
      ],
      "args": [
        "./node_modules/.bin/tape",
        "test/**/*.test.ts"
      ],
      "name": "debug-haiku-formats-tests"
    }
  ]
}
