{
  "version": "2.1.48",
  "build": {
    "appId": "com.Haiku.HaikuForDesignersAndEngineers",
    "copyright": "Copyright (c) 2017 Haiku",
    "productName": "Haiku",
    "forceCodeSigning": true,
    "electronVersion": "1.7.0",
    "asar": true,
    "asarUnpack": [
      "index.js",
      "config.js",
      "bins/*",
      "libs/plumbing/HaikuHelper.js",
      "libs/plumbing/bins/*",
      "libs/plumbing/lib/Plumbing.js",
      "libs/plumbing/lib/**/*",
      "libs/plumbing/node_modules/haiku-cli/*.js",
      "libs/plumbing/node_modules/haiku-cli/lib/*.js",
      "libs/plumbing/node_modules/haiku-creator-electron/index.html",
      "libs/plumbing/node_modules/haiku-creator-electron/*.js",
      "libs/plumbing/node_modules/haiku-creator-electron/public/**/*",
      "libs/plumbing/node_modules/haiku-creator-electron/react/**/*",
      "libs/plumbing/node_modules/haiku-creator-electron/utils/*",
      "libs/plumbing/node_modules/haiku-glass/index.html",
      "libs/plumbing/node_modules/haiku-glass/public/**/*",
      "libs/plumbing/node_modules/haiku-glass/react/**/*",
      "libs/plumbing/node_modules/haiku-timeline/index.html",
      "libs/plumbing/node_modules/haiku-timeline/public/**/*",
      "libs/plumbing/node_modules/haiku-timeline/src/**/*",
      "libs/plumbing/node_modules/@haiku/player/package.json",
      "libs/plumbing/node_modules/@haiku/player/src/*",
      "libs/plumbing/node_modules/@haiku/player/src/**/*",
      "libs/plumbing/node_modules/haiku-serialization/src/**/*.js",
      "libs/plumbing/node_modules/haiku-serialization/node_modules/@haiku/player/src/*",
      "libs/plumbing/node_modules/haiku-serialization/node_modules/@haiku/player/src/**/*"
    ],
    "mac": {
      "category": "public.app-category.developer-tools"
    },
    "files": [
      "**/*",
      "!**/node_modules/*/{CHANGELOG.md,README.md,README,readme.md,readme,test,__tests__,tests,powered-test,example,examples,*.d.ts}",
      "!**/node_modules/.bin",
      "!**/*.{o,hprof,orig,pyc,pyo,rbc}",
      "!**/._*",
      "!**/{.DS_Store,.git,.hg,.svn,CVS,RCS,SCCS,__pycache__,thumbs.db,.gitignore,.gitattributes,.editorconfig,.flowconfig,.yarn-metadata.json,.idea,appveyor.yml,.travis.yml,circle.yml,npm-debug.log,yarn-debug.log,.nyc_output,yarn.lock,.yarn-integrity}",
      "!libs/*/{CHANGELOG.md,README.md,README,readme.md,readme,test,__tests__,tests,powered-test,example,examples,*.d.ts}",
      "!libs/plumbing/node_modules/nodegit",
      "!libs/plumbing/node_modules/electron",
      "libs/plumbing/node_modules/nodegit/package.json",
      "libs/plumbing/node_modules/nodegit/dist/**",
      "libs/plumbing/node_modules/nodegit/build/Release/nodegit.node",
      "!build${/*}",
      "!certs${/*}",
      "!scripts${/*}",
      "!test${/*}",
      "!deploy.js",
      "!pipeline.js",
      "!local-install.sh",
      "!releases.json",
      "!README.md"
    ]
  },
  "engines": {
    "node": "8.4.0",
    "npm": "5.3.0",
    "yarn": "0.27.5",
    "electron": "1.7.0"
  },
  "name": "haiku-distro",
  "private": true,
  "description": "Scripts and config to build+distribute Haiku",
  "main": "index.js",
  "author": "Matthew Trost <matthew@haiku.ai>",
  "license": "PROPRIETARY",
  "scripts": {
    "start": "node ./pipeline.js",
    "test": "tape \"test/**/*.test.js\" | tap-spec",
    "lint": "standard --fix --verbose \"{./scripts/*.js,./pipeline.js}\" | snazzy",
    "open": "open ./dist/mac/Haiku.app/Contents/MacOS/Haiku"
  },
  "devDependencies": {
    "async": "2.1.4",
    "aws-sdk": "2.7.21",
    "babel-cli": "^6.24.0",
    "babel-preset-react-app": "^2.2.0",
    "electron-builder": "17.5.0",
    "electron-rebuild": "1.5.11",
    "fs-extra": "2.0.0",
    "git-branch": "0.3.0",
    "glob": "7.1.1",
    "glob-all": "3.1.0",
    "inquirer": "^3.0.6",
    "lodash": "^4.17.4",
    "mime-types": "2.1.14",
    "moment": "2.17.1",
    "snazzy": "^7.0.0",
    "standard": "^10.0.2",
    "tap-spec": "4.1.1",
    "tape": "4.6.3",
    "uglify-js": "mishoo/UglifyJS2.git#52ce9a333c78ba372a995f56c3b0352a7b08f6e8",
    "yargs": "6.6.0"
  },
  "dependencies": {
    "handlebars": "^4.0.8",
    "slack": "^8.3.1"
  }
}
