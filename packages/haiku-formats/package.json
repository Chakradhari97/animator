{
  "name": "haiku-formats",
  "version": "2.3.49",
  "description": "Formatting-related utilities for Haiku.",
  "main": "lib/index.js",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "lint": "tslint -p tsconfig.json -c tslint.json --type-check --format stylish",
    "fix": "yarn lint --fix",
    "test": "tsc && tsc -p tsconfig.test.json && NODE_ENV=test tape \"~test/**/*.test.js\" | tap-spec && rm -rf ~test",
    "coverage": "nyc yarn test",
    "html-coverage": "nyc --reporter=html yarn test",
    "compile": "tsc",
    "develop": "tsc --watch"
  },
  "authors": [
    "Sasha Joseph <sasha@haiku.ai>"
  ],
  "license": "UNLICENSED",
  "dependencies": {
    "bezier-easing": "^2.0.3",
    "haiku-common": "HaikuTeam/common.git",
    "@haiku/player": "HaikuTeam/player.git",
    "haiku-sdk-creator": "HaikuTeam/sdk-creator.git",
    "haiku-serialization": "HaikuTeam/serialization.git",
    "lodash": "^4.17.4"
  },
  "devDependencies": {
    "@types/node": "^8.0.34",
    "@types/tape": "^4.2.30",
    "ajv": "^5.2.3",
    "bodymovin": "4.11.1",
    "haiku-testing": "HaikuTeam/testing.git",
    "nyc": "^11.2.1",
    "tap-spec": "^4.1.1",
    "tape": "^4.7.0",
    "tslint": "^5.7.0",
    "tslint-config-haiku": "^1.0.0",
    "typescript": "^2.4.2",
    "walk-sync": "^0.3.2"
  }
}
