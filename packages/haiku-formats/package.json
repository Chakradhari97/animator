{
  "name": "haiku-formats",
  "version": "3.3.4",
  "description": "Formatting-related utilities for Haiku.",
  "main": "lib/index.js",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "lint": "tslint -p tsconfig.all.json -t stylish",
    "lint-report": "yarn lint -t checkstyle -o checkstyle-result.xml",
    "fix": "yarn lint --fix",
    "ts": "ts-node -r tsconfig-paths/register -P tsconfig.all.json",
    "test": "yarn ts ./node_modules/.bin/tape 'test/**/*.test.ts' | tap-spec",
    "test-report": "nyc yarn ts ./node_modules/.bin/tape 'test/**/*.test.ts' > test-result.tap",
    "compile": "tsc",
    "develop": "tsc --watch",
    "regenerate-goldens": "yarn ts ./scripts/regenerate-goldens.ts",
    "upgrade-goldens": "yarn ts ./scripts/upgrade-goldens.ts",
    "demos": "node ./scripts/demos.js"
  },
  "authors": [
    "Sasha Joseph <sasha@haiku.ai>"
  ],
  "license": "UNLICENSED",
  "dependencies": {
    "@haiku/core": "3.3.4",
    "bezier-easing": "^2.0.3",
    "haiku-common": "3.3.4",
    "haiku-sdk-creator": "3.3.4",
    "haiku-serialization": "3.3.4",
    "lodash": "^4.17.4"
  },
  "devDependencies": {
    "@types/async": "^2.0.49",
    "@types/bezier-easing": "^2.0.0",
    "@types/lodash": "^4.14.108",
    "@types/node": "8.9.4",
    "@types/tape": "^4.2.30",
    "async": "^2.5.0",
    "cross-env": "^5.1.4",
    "haiku-fs-extra": "3.3.4",
    "haiku-testing": "3.3.4",
    "istanbul-reporter-cobertura-haiku": "^1.0.1",
    "lottie-web": "5.1.7",
    "nodemon": "1.11.0",
    "nyc": "^11.8.0",
    "tap-spec": "^4.1.1",
    "tape": "^4.8.0",
    "ts-node": "^6.0.3",
    "tsconfig-paths": "^3.3.2",
    "tslint": "^5.7.0",
    "tslint-config-haiku": "^1.0.0",
    "typescript": "^2.5.2",
    "walk-sync": "^0.3.2",
    "webpack": "^3.8.1",
    "webpack-concat-plugin": "^2.4.2"
  }
}
