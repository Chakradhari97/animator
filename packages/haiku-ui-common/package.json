{
  "name": "haiku-ui-common",
  "version": "2.3.62",
  "description": "Common UI components for Haiku projects.",
  "main": "lib/index.js",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "lint": "tslint -p tsconfig.json -c tslint.json --type-check --format stylish",
    "fix": "yarn lint --fix",
    "test": "tsc && tsc -p tsconfig.test.json && NODE_ENV=test tape \"~test/**/*.test.js\" | tap-spec && rm -rf ~test",
    "coverage": "nyc yarn test",
    "html-coverage": "nyc --reporter=html yarn test",
    "compile": "tsc",
    "develop": "tsc --watch"
  },
  "authors": [
    "Sasha Joseph <sasha@haiku.ai>"
  ],
  "devDependencies": {
    "@types/node": "^8.5.2",
    "@types/react": "^15.4.2",
    "haiku-testing": "2.3.62",
    "haiku-common": "2.3.62",
    "nyc": "^11.3.0",
    "tap-spec": "^4.1.1",
    "tape": "^4.8.0",
    "tslint": "^5.7.0",
    "tslint-config-haiku": "^1.0.0",
    "typescript": "^2.5.2"
  },
  "license": "UNLICENSED",
  "dependencies": {
    "color": "^2.0.1",
    "decamelize": "^1.2.0",
    "lodash.assign": "^4.2.0",
    "numeral": "^2.0.6",
    "radium": "0.18.1",
    "react": "15.4.2"
  }
}
