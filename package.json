  "devDependencies": {
    "async": "2.1.5",
    "aws-sdk": "^2.85.0",
    "browserify": "^14.4.0",
    "cli-color": "1.2.0",
    "dependency-graph": "^0.5.0",
    "derequire": "^2.0.6",
    "fs-extra": "2.0.0",
