{
  "name": "haiku-interpreter",
  "version": "1.0.0",
  "description": "Haiku bytecode interpreter (a.k.a. the player)",
  "main": "index.js",
  "scripts": {
    "test": "tape \"test/**/*.test.js\" | tap-spec",
    "lint": "standard \"src/**/*.js\" --fix --verbose | snazzy",
    "demo": "node ./server.js",
    "pretty": "prettier",
    "dist:dom": "browserify ./src/creation/dom/index.js --standalone HaikuDOMPlayer | derequire > dom.bundle.js && uglifyjs dom.bundle.js -o dom.bundle.js",
    "dist:react": "cp ./src/adapters/react/index.js react.bundle.js"
  },
  "author": "Matthew Trost <matthew@haiku.ai>",
  "license": "UNLICENSED",
  "devDependencies": {
    "async": "^2.3.0",
    "browserify": "14.1.0",
    "derequire": "2.0.6",
    "express": "4.14.1",
    "fs-extra": "2.0.0",
    "handlebars": "4.0.6",
    "jsdom": "9.11.0",
    "prettier": "^0.22.0",
    "react-dom": "15.4.2",
    "snazzy": "6.0.0",
    "standard": "8.6.0",
    "tap-spec": "4.1.1",
    "tape": "4.6.3",
    "uglify-js": "2.7.5"
  },
  "dependencies": {
    "haiku-bytecode": "git@github.com:HaikuTeam/bytecode#60e5125923c5697d788246e578f1e2f90c7a2cc2",
    "lodash.assign": "^4.2.0",
    "lodash.merge": "^4.6.0",
    "raf": "3.3.0",
    "react": "15.4.2",
    "react-dom": "15.4.2",
    "react-test-renderer": "^15.4.2"
  }
}
