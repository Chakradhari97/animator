{
  "name": "haiku-mono",
  "private": true,
  "version": "2.3.5",
  "description": "Haiku monorepo",
  "main": "index.js",
  "author": "Matthew Trost <matthew@trost.co>",
  "license": "UNLICENSED",
  "engines": {
    "node": "8.4.0",
    "npm": "5.3.0",
    "yarn": "1.0.2",
    "electron": "1.7.0"
  },
  "scripts": {
    "go": "yarn start -- --default",
    "setup": "node ./scripts/setup.js",
    "changelog": "node ./scripts/changelog.js",
    "start": "node ./scripts/start.js",
    "finish": "node ./scripts/finish.js",
    "lint": "yarn run lint-scripts && yarn run lint-distro-scripts",
    "lint-scripts": "standard \"./scripts/**/*.js\" --fix --verbose | snazzy",
    "lint-distro-scripts": "standard \"./distro-scripts/**/*.js\" --fix --verbose | snazzy",
    "semver": "node ./scripts/semver.js",
    "lint-all": "node ./scripts/lint-all.js",
    "test-all": "node ./scripts/test-all.js",
    "git-subtree-push": "node ./scripts/git-subtree-push.js",
    "git-subtree-pull": "node ./scripts/git-subtree-pull.js",
    "yarn-install": "node ./scripts/yarn-install.js",
    "yarn-link": "node ./scripts/yarn-link.js",
    "yarn-unlink": "node ./scripts/yarn-unlink.js",
    "distro": "node ./distro.js",
    "distro-open": "open ./dist/mac/Haiku.app/Contents/MacOS/Haiku"
  },
  "devDependencies": {
    "async": "2.1.5",
    "aws-sdk": "^2.85.0",
    "babel-cli": "^6.24.0",
    "babel-preset-react-app": "^2.2.0",
    "browserify": "^14.4.0",
    "cli-color": "1.2.0",
    "dependency-graph": "^0.5.0",
    "derequire": "^2.0.6",
    "electron-builder": "17.5.0",
    "electron-rebuild": "1.5.11",
    "fs-extra": "2.0.0",
    "git-branch": "0.3.0",
    "glob": "7.1.1",
    "glob-all": "3.1.0",
    "inquirer": "^3.0.6",
    "lodash": "^4.17.4",
    "mime-types": "^2.1.15",
    "moment": "2.17.1",
    "semver": "5.3.0",
    "semver-sort": "^0.0.4",
    "snazzy": "^7.0.0",
    "standard": "^10.0.2",
    "tap-spec": "4.1.1",
    "tape": "4.6.3",
    "uglify-js": "mishoo/UglifyJS2.git#52ce9a333c78ba372a995f56c3b0352a7b08f6e8",
    "yargs": "6.6.0"
  },
  "standard": {
    "ignore": [
      "/packages/*"
    ]
  },
  "dependencies": {
    "handlebars": "^4.0.8",
    "slack": "^8.3.1"
  },
  "build": {
    "appId": "com.Haiku.HaikuForDesignersAndEngineers",
    "copyright": "Copyright (c) 2017 Haiku",
    "productName": "Haiku",
    "forceCodeSigning": true,
    "electronVersion": "1.7.0",
    "asar": true,
    "asarUnpack": [
      "index.js",
      "config.js",
      "bins/*",
      "packages/haiku-plumbing/HaikuHelper.js",
      "packages/haiku-plumbing/bins/*",
      "packages/haiku-plumbing/lib/Plumbing.js",
      "packages/haiku-plumbing/lib/**/*",
      "packages/haiku-plumbing/node_modules/haiku-cli/*.js",
      "packages/haiku-plumbing/node_modules/haiku-cli/lib/*.js",
      "packages/haiku-plumbing/node_modules/haiku-creator-electron/index.html",
      "packages/haiku-plumbing/node_modules/haiku-creator-electron/*.js",
      "packages/haiku-plumbing/node_modules/haiku-creator-electron/public/**/*",
      "packages/haiku-plumbing/node_modules/haiku-creator-electron/react/**/*",
      "packages/haiku-plumbing/node_modules/haiku-creator-electron/utils/*",
      "packages/haiku-plumbing/node_modules/haiku-glass/index.html",
      "packages/haiku-plumbing/node_modules/haiku-glass/public/**/*",
      "packages/haiku-plumbing/node_modules/haiku-glass/react/**/*",
      "packages/haiku-plumbing/node_modules/haiku-timeline/index.html",
      "packages/haiku-plumbing/node_modules/haiku-timeline/public/**/*",
      "packages/haiku-plumbing/node_modules/haiku-timeline/src/**/*",
      "packages/haiku-plumbing/node_modules/@haiku/player/package.json",
      "packages/haiku-plumbing/node_modules/@haiku/player/src/*",
      "packages/haiku-plumbing/node_modules/@haiku/player/src/**/*",
      "packages/haiku-plumbing/node_modules/@haiku/player/lib/*",
      "packages/haiku-plumbing/node_modules/@haiku/player/lib/**/*",
      "packages/haiku-plumbing/node_modules/haiku-serialization/src/**/*.js",
      "packages/haiku-plumbing/node_modules/haiku-serialization/node_modules/@haiku/player/src/*",
      "packages/haiku-plumbing/node_modules/haiku-serialization/node_modules/@haiku/player/src/**/*",
      "packages/haiku-plumbing/node_modules/haiku-serialization/node_modules/@haiku/player/lib/*",
      "packages/haiku-plumbing/node_modules/haiku-serialization/node_modules/@haiku/player/lib/**/*"
    ],
    "mac": {
      "category": "public.app-category.developer-tools"
    },
    "files": [
      "**/*",
      "!**/node_modules/*/{CHANGELOG.md,README.md,README,readme.md,readme,test,__tests__,tests,powered-test,example,examples,*.d.ts}",
      "!**/node_modules/.bin",
      "!**/*.{o,hprof,orig,pyc,pyo,rbc}",
      "!**/._*",
      "!**/{.DS_Store,.git,.hg,.svn,CVS,RCS,SCCS,__pycache__,thumbs.db,.gitignore,.gitattributes,.editorconfig,.flowconfig,.yarn-metadata.json,.idea,appveyor.yml,.travis.yml,circle.yml,npm-debug.log,yarn-debug.log,.nyc_output,yarn.lock,.yarn-integrity}",
      "!packages/*/{CHANGELOG.md,README.md,README,readme.md,readme,test,__tests__,tests,powered-test,example,examples,*.d.ts}",
      "!packages/haiku-plumbing/node_modules/nodegit",
      "!packages/haiku-plumbing/node_modules/electron",
      "packages/haiku-plumbing/node_modules/nodegit/package.json",
      "packages/haiku-plumbing/node_modules/nodegit/dist/**",
      "packages/haiku-plumbing/node_modules/nodegit/build/Release/nodegit.node",
      "!build${/*}",
      "!certs${/*}",
      "!changelog${/*}",
      "!distro-scripts${/*}",
      "!packages/haiku-bytecode/${/*}",
      "!packages/haiku-cli/${/*}",
      "!packages/haiku-creator/${/*}",
      "!packages/haiku-glass/${/*}",
      "!packages/haiku-player/${/*}",
      "!packages/haiku-sdk-client/${/*}",
      "!packages/haiku-sdk-creator/${/*}",
      "!packages/haiku-sdk-inkstone/${/*}",
      "!packages/haiku-serialization/${/*}",
      "!packages/haiku-state-object/${/*}",
      "!packages/haiku-timeline/${/*}",
      "!packages/haiku-websockets/${/*}",
      "!scripts${/*}",
      "!test${/*}",
      "!_config.js.handlebars",
      "!deploy.js",
      "!distro.js",
      "!local-install.sh",
      "!README.md",
      "!releases.json",
      "!yarn.lock"
    ]
  }
}
