{
  "name": "haiku-interpreter",
  "version": "2.0.102",
  "description": "Haiku bytecode interpreter (a.k.a. the player)",
  "main": "index.js",
  "scripts": {
    "test": "tape \"test/**/*.test.js\" | tap-spec || true",
    "lint": "standard \"src/**/*.js\" --fix --verbose | snazzy || true",
    "demo": "node ./server.js",
    "pretty": "prettier",
    "build-and-copy": "npm run dist:dom && npm run dist:react && cp dom.bundle.js `pwd`/../haiku-npm/at-haiku-player/dom/index.js && cp react.bundle.js `pwd`/../haiku-npm/at-haiku-player/dom/react.js",
    "dist:dom": "browserify ./src/creation/dom/index.js --standalone HaikuDOMPlayer | derequire > dom.bundle.js",
    "dist:react": "browserify ./src/adapters/react/index.js --standalone HaikuReactAdapter --external react --external react-test-renderer --external lodash.merge | derequire > react.bundle.js && sed -i '' -E -e \"s/_dereq_[(]'(react|react-test-renderer|lodash\\.merge)'[)]/require('\\1')/g\" react.bundle.js"
  },
  "author": "Matthew Trost <matthew@haiku.ai>",
  "license": "UNLICENSED",
  "devDependencies": {
    "async": "^2.3.0",
    "browserify": "14.1.0",
    "derequire": "2.0.6",
    "express": "4.14.1",
    "fs-extra": "2.0.0",
    "handlebars": "4.0.6",
    "jsdom": "9.11.0",
    "leaked-handles": "^5.2.0",
    "prettier": "0.22.0",
    "react-dom": "15.4.2",
    "snazzy": "6.0.0",
    "standard": "8.6.0",
    "tap-spec": "4.1.1",
    "tape": "4.6.3",
    "uglify-js": "2.7.5"
  },
  "dependencies": {
    "haiku-bytecode": "git@github.com:HaikuTeam/bytecode#74f2d6f8056c3ce45205dfd17bc9bdd84f1a71ff",
    "lodash.assign": "4.2.0",
    "lodash.merge": "4.6.0",
    "mixpanel-browser": "^2.11.1",
    "raf": "^3.3.0",
    "react": "15.4.2",
    "react-dom": "15.4.2",
    "react-test-renderer": "15.4.2"
  }
}
