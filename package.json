    "mono:changelog": "node ./scripts/changelog.js",
    "mono:lint-all": "node ./scripts/lint-all.js",
    "mono:test-all": "node ./scripts/test-all.js",
    "mono:finalize": "node ./scripts/finalize.js",
    "mono:finish": "node ./scripts/finish.js",
    "mono:cmd": "node ./scripts/cmd.js",
    "mono:yarn-clean": "node ./scripts/yarn-clean.js",
    "mono:yarn-install": "node ./scripts/yarn-install.js",
    "mono:yarn-link": "node ./scripts/yarn-link.js",
    "mono:yarn-unlink": "node ./scripts/yarn-unlink.js",
    "mono:yarn-semver-set": "node ./scripts/yarn-semver-set.js",
